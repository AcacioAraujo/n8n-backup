              "name": "Phone",
              "value": "={{ \n(()=>{\n  let phoneNumber = $json.telefone.toString()\n  // Verifica se tem 14 caracteres e come√ßa com +\n  if (phoneNumber.length === 14 && phoneNumber[0] === '+') {\n      // Remove o + e continua o processamento com o resto do n√∫mero\n      phoneNumber = phoneNumber.substring(1);\n  }\n\n  // Extrai o DDD (posi√ß√µes 2 e 3 do n√∫mero)\n  const ddd = parseInt(phoneNumber.substring(2, 4));\n\n  // Verifica se o DDD est√° nos intervalos especificados\n  const isSpecialDDD = (ddd >= 11 && ddd <= 19) || \n                      (ddd >= 21 && ddd <= 24) || \n                      (ddd >= 27 && ddd <= 28);\n\n  if (isSpecialDDD) {\n      return phoneNumber;\n  } else {\n      // Remove o d√≠gito 9 da 5¬™ posi√ß√£o (√≠ndice 4)\n      return phoneNumber.substring(0, 4) + phoneNumber.substring(5);\n  }\n})()\n}}",
